	"license": "MIT",
	"homepage": "https://github.com/pluv-io/pluv",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/pluv-io/pluv.git",
		"directory": "packages/types"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"publishConfig": {
