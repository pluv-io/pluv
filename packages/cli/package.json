		"execa": "^7.1.1",
		"fs-extra": "^11.1.1",
		"gradient-string": "^2.0.2",
		"inquirer": "^9.2.3",
		"kleur": "^4.1.5",
		"ora": "^6.3.1",
		"type-fest": "^3.10.0"
	},
