{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "CLI",
	"extends": "./base.json",
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"jsx": "react",
		"lib": ["DOM", "DOM.Iterable", "ES2020"],
        "module": "nodenext",
        "moduleResolution": "nodenext",
		"moduleDetection": "force",
		"noEmit": false,
		"outDir": "./dist",
		"resolveJsonModule": false,
        "target": "es2021"
	}
}
