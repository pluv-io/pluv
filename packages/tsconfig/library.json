{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Library",
	"extends": "./base.json",
	"compilerOptions": {
		"lib": ["esnext"],
		"module": "esnext",
		"target": "es6",
		"isolatedModules": true,
		"moduleResolution": "bundler"
	},
	"include": ["src"],
	"exclude": ["node_modules"]
}
