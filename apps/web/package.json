        "@types/glob": "^8.1.0",
        "autoprefixer": "^10.4.14",
        "babel-loader": "^9.1.2",
        "babel-plugin-macros": "^3.1.0",
        "babel-plugin-styled-components": "^2.1.1",
        "concurrently": "^8.0.1",
        "dotenv-cli": "^7.2.1",
        "eslint": "8.40.0",
