{
    "name": "@pluv-apps/web",
    "version": "0.0.0",
    "private": true,
    "sideEffects": [
        "*.css"
    ],
    "scripts": {
        "build": "pnpm run prepare && dotenv -- next build",
        "build:demo": "pnpm run script -- scripts/generate-demo-data",
        "build:docs": "pnpm run script -- scripts/generate-docs",
        "dev": "dotenv -- next dev -p 3001",
        "lint": "next lint --fix",
        "prepare": "concurrently \"pnpm:build:demo\" \"pnpm:build:docs\"",
        "script": "dotenv -- ts-node -P tsconfig.script.json -T",
        "start": "serve out",
        "tsc:check": "tsc --noEmit"
    },
    "files": [
        "components.ts",
        "pluv-io.ts"
    ],
    "dependencies": {
        "@dnd-kit/core": "^6.3.1",
        "@mdx-js/react": "^3.1.0",
        "@pluv-apps/server-cloudflare": "workspace:^",
        "@pluv-internal/mdx-components": "workspace:^",
        "@pluv-internal/react-components": "workspace:^",
        "@pluv-internal/react-hooks": "workspace:^",
        "@pluv-internal/react-icons": "workspace:^",
        "@pluv-internal/remark-plugins": "workspace:^",
        "@pluv/crdt-yjs": "workspace:^",
        "@pluv/io": "workspace:^",
        "@pluv/platform-pluv": "workspace:^",
        "@pluv/react": "workspace:^",
        "@radix-ui/react-tooltip": "^1.1.8",
        "@seznam/compose-react-refs": "^1.0.6",
        "@shikijs/twoslash": "^2.3.2",
        "@tanstack/react-table": "^8.21.2",
        "@types/common-tags": "^1.8.4",
        "@types/mdx": "^2.0.13",
        "@types/ms": "^2.1.0",
        "@types/node": "^22.13.4",
        "@types/react": "^19.0.8",
        "@types/react-dom": "^19.0.3",
        "@types/ws": "^8.5.14",
        "clsx": "^2.1.1",
        "common-tags": "^1.8.2",
        "gray-matter": "^4.0.3",
        "ms": "^2.1.3",
        "next": "^15.1.7",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "shiki": "^2.3.2",
        "yjs": "^13.6.23",
        "zod": "^3.24.2"
    },
    "devDependencies": {
        "@faker-js/faker": "^9.5.0",
        "@mdx-js/loader": "^3.1.0",
        "@next/bundle-analyzer": "^15.1.7",
        "@next/mdx": "^15.1.7",
        "@pluv-internal/tailwind-config": "workspace:^",
        "@pluv-internal/typings": "workspace:^",
        "@pluv-internal/utils": "workspace:^",
        "@pluv/io": "workspace:^",
        "@pluv/platform-node": "workspace:^",
        "@pluv/tsconfig": "workspace:^",
        "@types/express": "^5.0.0",
        "@types/fs-extra": "^11.0.4",
        "@types/glob": "^8.1.0",
        "autoprefixer": "^10.4.20",
        "concurrently": "^9.1.2",
        "dotenv-cli": "^8.0.0",
        "eslint": "^8.57.1",
        "eslint-config-pluv": "workspace:^",
        "fs-extra": "^11.3.0",
        "glob": "^11.0.1",
        "next-compose-plugins": "^2.2.1",
