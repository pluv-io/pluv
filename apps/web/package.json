        "@types/styled-components": "^5.1.26",
        "@types/ws": "^8.5.4",
        "clsx": "^1.2.1",
        "common-tags": "^1.8.2",
        "gray-matter": "^4.0.3",
        "ms": "^2.1.3",
        "next": "13.4.2",
        "polished": "^4.2.2",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "styled-components": "^5.3.10",
        "twin.macro": "^3.3.1",
        "yjs": "^13.6.0",
