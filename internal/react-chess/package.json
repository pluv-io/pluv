	},
	"dependencies": {
		"@pluv-internal/react-hooks": "workspace:^",
		"@types/react": "18.0.27",
		"chess.js": "1.0.0-beta.1",
		"react": "18.2.0",
		"react-chessboard": "^2.0.8"
	},
	"devDependencies": {
		"@pluv/tsconfig": "workspace:^",
		"eslint-config-pluv": "workspace:^",
		"tsup": "^6.5.0",
		"typescript": "^4.9.4"
	}
}
